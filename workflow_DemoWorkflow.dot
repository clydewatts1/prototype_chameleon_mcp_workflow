digraph "DemoWorkflow" {
  label="DemoWorkflow";
  labelloc="t";
  node [fontname="Arial"];
  edge [fontname="Arial"];

  role_Initiator [label="Initiator", shape=circle, style=filled, fillcolor=lightblue];
  role_Reviewer [label="Reviewer", shape=circle, style=filled, fillcolor=lightblue];
  role_Finalizer [label="Finalizer", shape=circle, style=filled, fillcolor=lightblue];
  interaction_PendingQueue [label="PendingQueue", shape=hexagon, style=filled, fillcolor=lightgreen];
  interaction_ReviewQueue [label="ReviewQueue", shape=hexagon, style=filled, fillcolor=lightgreen];
  interaction_ApprovalQueue [label="ApprovalQueue", shape=hexagon, style=filled, fillcolor=lightgreen];
  guardian_ComplianceCheck [label="ComplianceCheck", shape=doubleoctagon, style=filled, fillcolor=salmon];
  guardian_ApprovalGate [label="ApprovalGate", shape=doubleoctagon, style=filled, fillcolor=salmon];

  role_Initiator -> interaction_PendingQueue [label="SubmitToPending"];
  interaction_PendingQueue -> guardian_ComplianceCheck [label="FetchFromPending"];
  guardian_ComplianceCheck -> role_Reviewer;
  role_Reviewer -> interaction_ReviewQueue [label="MoveToReview"];
  interaction_ReviewQueue -> guardian_ApprovalGate [label="FetchFromReview"];
  guardian_ApprovalGate -> role_Finalizer;
  role_Finalizer -> interaction_ApprovalQueue [label="MoveToApproval"];
}